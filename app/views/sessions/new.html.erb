
<h1>Welcome! Please login </h1>
<div class="login">
   <%= form_tag login_path do %>
      <%= email_field_tag :email,nil, class:'form-control',placeholder: "email"%><br>
      <%= password_field_tag :password,nil, class:'form-control',placeholder: "password"%><br>
      <%= submit_tag "Login", class:'form-control bg-info'%>
   <%end%>
</div>